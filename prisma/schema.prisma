// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Members {
  id       Int     @id @default(autoincrement())
  name     String
  role     String
  picture  String?
  isActive Boolean @default(true)
}
